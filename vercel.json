{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.dawnbudsmodelschool.com" }],
      "destination": "https://dawnbudsmodelschool.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
